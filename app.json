{
  "name": "mybot",
  "description": "Slack Bot written in Go",
  "keywords": [
    "go",
    "slack"
  ],
  "image": "heroku/go",
  "mount_dir": "src/github.com/rapidloop/mybot",
  "website": "https://www.opsdash.com/blog/slack-bot-in-golang.html",
  "repository": "https://github.com/rapidloop/mybot",
   "env": {
     "SLACK_BOT_TOKEN": {
       "description": "See https://<YOUR ORG>.slack.com/services/new/bot",
       "value": "CHANGEME"
     },
     "GO15VENDOREXPERIMENT": {
       "value": "1"
     }
   }
}
